{{!-- web/templates/financials.hbs --}}
{{!-- Financials --}}

<span class="centered">
    {{#if model}}
        <h3>Financials</h3>
        <table id="financials-table" class="display"></table>
        {{view "financials"}}
    {{else}}
        <span class="centered">
            <h4> This company has not provided it's financials in the SEC's XBRL format. </h4>
        </span>
    {{/if}}
</span>
